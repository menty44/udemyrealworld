<h1>Sign in</h1>

<%= form_for @conn, Routes.session_path(@conn, :create), [method: :post, as: :user], fn f -> %>

  <%= label f, :email %>
  <%= text_input f, :email %>
  <%= error_tag f, :email %>

  <%= label f, :password %>
  <%= text_input f, :password %>

  <%= submit "Login"%>
<% end %>

<%= #form_for @conn, Routes.session_path(@conn, :delete), [method: :delete, as: :user], fn f -> %>
  <%= #f.submit "Logout"%>
<% #end %>
